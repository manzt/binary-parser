{
  "name": "binary-parser",
  "version": "1.9.2",
  "description": "Blazing-fast binary parser builder",
  "main": "dist/binary_parser.js",
  "module": "dist/binary_parser.mjs",
  "devDependencies": {
    "@types/node": "^16.9.1",
    "esbuild": "^0.13.2",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.4.0",
    "typescript": "^4.4.3"
  },
  "exports": {
    ".": {
      "import": "./dist/binary_parser.mjs",
      "require": "./dist/binary_parser.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "run-p build:types build:cjs build:esm",
    "build:types": "tsc --emitDeclarationOnly",
    "build:cjs": "esbuild --bundle --format=cjs --outfile=dist/binary_parser.js lib/binary_parser.ts",
    "build:esm": "esbuild --bundle --format=esm --outfile=dist/binary_parser.mjs lib/binary_parser.ts",
    "fmt": "prettier --write \"{lib,example,test,benchmark}/**/*.{ts,js}\"",
    "check-fmt": "prettier --list-different \"{lib,example,test,benchmark}/**/*.{ts,js}\"",
    "test": "mocha",
    "test-browser": "parcel test/browser.html --open",
    "cover": "nyc --reporter html mocha",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*.{js,mjs,d.ts}"
  ],
  "keywords": [
    "binary",
    "parser",
    "decode",
    "unpack",
    "struct",
    "buffer",
    "bit"
  ],
  "author": {
    "name": "Keichi Takahashi",
    "email": "keichi.t@me.com",
    "url": "https://keichi.dev/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/keichi/binary-parser.git"
  },
  "bugs": "http://github.com/keichi/binary-parser/issues",
  "engines": {
    "node": ">=12"
  }
}
